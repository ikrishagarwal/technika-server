{
  "name": "technika25-server",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "scripts": {
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "watch:start": "nodemon -w ./dist --enable-source-maps dist/server.js",
    "start": "pnpm run build:ts && node dist/server.js",
    "dev": "pnpm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"pnpm:watch:ts\" \"pnpm:dev:start\"",
    "dev:start": "node --enable-source-maps dist/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.6.2",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@sentry/node": "^10.30.0",
    "dotenv": "^16.1.4",
    "fastify": "^5.0.0",
    "firebase-admin": "^13.6.0",
    "nanoid": "^5.1.6",
    "pino-pretty": "^13.1.3",
    "ws": "^8.13.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^24.0.8",
    "@types/ws": "^8.5.4",
    "concurrently": "^9.0.0",
    "fastify-tsconfig": "^3.0.0",
    "typescript": "~5.9.2"
  }
}
